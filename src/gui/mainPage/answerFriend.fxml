<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="popup" prefHeight="85.0" prefWidth="333.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.mainPage.AnswerFriend">
    <children>
        <Label layoutX="56.0" layoutY="14.0" prefHeight="15.0" prefWidth="221.0" text="Do you want to accept the friendship?" textAlignment="CENTER" />
        <Button fx:id="accept" layoutX="85.0" layoutY="38.0" mnemonicParsing="false" prefHeight="10.0" prefWidth="40.0" styleClass="buttonYesNo" text="Yes">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding></Button>
        <Button fx:id="decline" layoutX="203.0" layoutY="38.0" mnemonicParsing="false" prefHeight="10.0" prefWidth="40.0" styleClass="buttonYesNo" text="No">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding></Button>
    </children>
</AnchorPane>
